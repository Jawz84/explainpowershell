{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run analysis service test",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/test.ps1",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        },
        {
            "name": "debug wasm",
            "type": "blazorwasm",
            "request": "launch",
            "cwd": "${workspaceFolder}/explainpowershell.frontend/",
            "browser": "edge"
        }
    ],
    "compounds": [
        {
            "name": "debug solution",
            "configurations": ["Attach to .NET Functions", "debug wasm"]
        }
    ]
}